<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane prefHeight="395.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab38.TemplateMethodController">
    <Pane fx:id="pane" prefHeight="330.0" prefWidth="600.0" />
    <Separator layoutY="325.0" prefHeight="16.0" prefWidth="600.0" />
    <ButtonBar layoutX="50.0" layoutY="335.0" prefHeight="40.0" prefWidth="500.0">
        <buttons>
            <ComboBox fx:id="comboBox" prefWidth="150.0" promptText="Мяч" />
            <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#onStartButtonClick" text="Пуск" />
            <Button alignment="CENTER" mnemonicParsing="false" onMouseClicked="#onExitButtonClick" prefHeight="26.0" text="Закрыть" />
        </buttons>
        <padding>
            <Insets bottom="10.0" right="10.0" top="10.0" />
        </padding>
        <opaqueInsets>
            <Insets />
        </opaqueInsets>
    </ButtonBar>
</AnchorPane>
